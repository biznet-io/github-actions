{
  "name": "@whoz/setup-working-directory-action",
  "version": "1.0.0",
  "description": "Sets up a working directory for GitHub Actions workflows",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && ncc build lib/index.js -o dist --source-map --license licenses.txt",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "all": "npm run format && npm run lint && npm run type-check && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoz/actions.git"
  },
  "keywords": [
    "github",
    "actions",
    "setup",
    "working-directory"
  ],
  "author": "Whoz",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
