{
  "name": "@whoz/init-action",
  "version": "1.0.0",
  "description": "Initialize repository with SSH setup, git configuration and caching",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build lib/index.js -o dist --source-map --license licenses.txt",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "all": "npm run format && npm run lint && npm run type-check && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoz/actions.git"
  },
  "keywords": [
    "github",
    "actions",
    "init",
    "repository",
    "git",
    "ssh"
  ],
  "author": "Whoz",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^9.21.0",
    "eslint-plugin-github": "^5.1.8",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
